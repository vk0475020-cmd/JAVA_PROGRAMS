import java.util.*;
class Solution {
    public int[] twoSum(int[] nums, int input) {
        for (int i = 0; i < nums.length; i++) {
            for (int j = i + 1; j < nums.length; j++) {
                if (nums[i] + nums[j] == input) {
                    return new int[] { i, j };
                }
            }
        }
        return null;//if be don't use then compilar genrate error!!!!
    }
}
